<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="210.0" prefWidth="440.0" spacing="10" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.VentanaModificarController">
    <HBox spacing="10">
        <Label prefHeight="17.0" prefWidth="60.0" text="   Nombre" />
        <TextField fx:id="txtNombre" prefHeight="25.0" prefWidth="344.0" />
    </HBox>
    <HBox spacing="10">
        <Label text="   Apellidos" />
        <TextField fx:id="txtApellidos" prefHeight="25.0" prefWidth="344.0" />
    </HBox>
    <HBox spacing="10">
        <Label prefHeight="17.0" prefWidth="59.0" text="       Edad" />
        <TextField fx:id="txtEdad" prefHeight="25.0" prefWidth="97.0" />
    </HBox>
    <HBox alignment="CENTER" prefHeight="53.0" prefWidth="461.0" spacing="10">
        <Button fx:id="btnGuardar" onAction="#guardar" text="Guardar" />
        <Button fx:id="btnCancelar" onAction="#cancelar" text="Cancelar" />
    </HBox>

</VBox>